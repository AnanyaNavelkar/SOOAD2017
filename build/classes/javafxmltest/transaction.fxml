<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxmltest.TransactionController">
   <children>
      <Label fx:id="transactions" layoutX="16.0" layoutY="83.0" prefHeight="374.0" prefWidth="608.0">
         <font>
            <Font size="14.0" />
         </font></Label>
      <RadioButton fx:id="mobileRecharge" layoutX="195.0" layoutY="36.0" mnemonicParsing="false" onAction="#mobileRechargeOnAction" text="Mobile recharge">
         <toggleGroup>
            <ToggleGroup fx:id="services" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="electricityBill" layoutX="332.0" layoutY="36.0" mnemonicParsing="false" onAction="#electricityBillOnAction" text="Electricity Bill" toggleGroup="$services" />
      <RadioButton fx:id="payAUser" layoutX="195.0" layoutY="59.0" mnemonicParsing="false" onAction="#payAUserOnAction" text="Pay a user" toggleGroup="$services" />
      <RadioButton fx:id="bookAMovie" layoutX="331.0" layoutY="59.0" mnemonicParsing="false" onAction="#bookAMovieOnAction" text="Book a movie" toggleGroup="$services" />
      <RadioButton fx:id="allTransactions" layoutX="269.0" layoutY="14.0" mnemonicParsing="false" onAction="#showAllTransactions" selected="true" text="All transactions" toggleGroup="$services" />
   </children>
</AnchorPane>
